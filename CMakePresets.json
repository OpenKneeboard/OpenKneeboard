{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 31
  },
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "architecture": "x64",
      "generator": "Visual Studio 17 2022",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "third-party/vcpkg/scripts/buildsystems/vcpkg.cmake",
        "VCPKG_TARGET_TRIPLET": "x64-windows-openkneeboard"
      }
    },
    {
      "name": "debug",
      "inherits": "default"
    },
    {
      "name": "release",
      "inherits": "default"
    }
  ],
  "buildPresets": [
    {
      "name": "Debug",
      "configurePreset": "debug",
      "configuration": "Debug",
      "jobs": 16
    },
    {
      "name": "Release",
      "configurePreset": "release",
      "configuration": "RelWithDebInfo",
      "jobs": 16
    }
  ]
}
